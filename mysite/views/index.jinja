				  {% set products_t = { 'FCP': product_fcp, 'DVR': product_dvr } %}
                  {% set includes = {
                    'DVR': '53 Titles, 14 Effects, 5 Transitions',
                    'FCP': '67 Titles + 5 Transitions'
                  } %}
                  {% set product_options = {
                      'DVR': {
                        'music': {
                          'trailer': True
                        }
                      },
                      'FCP': {
                        'music': {
                          'trailer': False,
                        }
                      }
                  } %}
                  
				<div class="tables-wrapper">
				{% for key, product in products_t.items() %}
				<div class="tw">
				
				<div class="product__heading__controls">
                    <div class="control"><</div>
                    <div class="product__heading-wrapper">
                        <div class="product__heading active">
                          {{ picture(shared_media_url, key|lower + '_icon', formats=["webp","png"], file_sizes=[180], width=40, height=40, img_class='software-icon', loading="lazy") }}
                          <p class="mt-0">{{ key }} version</p>
                        </div>
                    </div>
                    <div class="control">></div>
				</div>
				
				<div class="table">
					<div class="cell label" class="td-right">Supported software</div>
					<div class="cell data" class="td-left">
						{% autoescape off %}
							{{ product.productdetails.minimal_requirements.replace('FCP 10', 'FCP\xa010').replace('Motion 5', 'Motion\xa05').replace(', ', ',<br>') }}
						{% endautoescape %}
					</div>
					<div class="cell label" class="td-right">Includes</div>
					<div class="cell data">{{ includes[key]|default('') }}</div>
					<div class="cell label" class="td-right">Size</div>
					<div class="cell data" class="td-left">{{ ((product.productdetails.source_size * 1000) | filesizeformat).replace('.0 ', ' ') }}</div>
					
					{% for kind in ['trailer', 'tutorial'] %}
					{% if not product_options[key].music[kind] %}
						{% with musics=product.productdetails.music.select_related('music_portal').all().filter(used_in_trailer=kind=='trailer') %}
							{% if musics|length %}
								<div class="cell label" class="td-right">Music used in {{ kind }}</div>
								<div class="cell data" class="td-left">
                    				{% for music in musics %}
            							{% if music.music_portal.name == "www.premiumbeat.com" or music.music_portal.name ==  "www.shutterstock.com" %}
            								<a href="{{ music.url }}" target="_blank" rel="noopener noreferrer">{{ music.music_portal.name.replace('www.','') }}</a> — {{ music.title }}{% if not loop.last %},&nbsp;<wbr>{% endif %}
            							{% elif music.music_portal.name == "epidemicsound.com" %}
            								<a href="https://www.epidemicsound.com/campaign/invite-a-creator/?_us=Referral&_usx=3zpfqv" target="_blank" rel="noopener noreferrer">{{ music.music_portal.name.replace('www.','') }}</a> — {{ music.title }}{% if not loop.last %},&nbsp;<wbr>{% endif %}
            							{% else %}
            								{{ music.music_portal.name.replace('www.','') }} — {{ music.title }}{% if not loop.last %},&nbsp;<wbr>{% endif %}
            							{% endif %}
            						{% endfor %}
								</div>
							{% endif %}
						{% endwith %}
					{% endif %}
					{% endfor %}
				</div>
				</div>
				{% endfor %}
				</div>
				
				<div class="minstaller-info">
					<h4>mInstaller</h4>
					<p>After purchasing, the product is&nbsp;<strong>instantly available</strong> for&nbsp;download via&nbsp;<a href="https://www.motionvfx.com/page/minstaller.html">mInstaller</a>.</p>
				</div>
			</div>

			<div class="extra">
				<picture>
					<source srcset="https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_gfx_7-640.webp 640w,
					https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_gfx_7-960.webp 960w,
					https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_gfx_7-1280.webp 1280w" type="image/webp" sizes="25vw">
					<source srcset="https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_gfx_7-640.png 640w,
					https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_gfx_7-960.png 960w,
					https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_gfx_7-1280.png 1280w" type="image/png" sizes="25vw">
					<img class="img-auto" src="https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_gfx_7-1280.png" alt="" width="1280" height="648" />
				</picture>
			</div>
		</section>


			<section class="footer center">
				<div class="image-background">
					<picture>
						<source srcset="https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_footer_1024x1250-640.jpg 640w,
							https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_footer_1024x1250-820.jpg 820w,
							https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_footer_1024x1250-1024.jpg 1024w"
							sizes="100vw" media="(max-width: 800px)">
						<source srcset="https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_footer-640.jpg 640w,
							https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_footer-960.jpg 960w,
							https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_footer-1280.jpg 1280w,
							https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_footer-1920.jpg 1920w,
							https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_footer-2560.jpg 2560w"
							sizes="100vw">
						<img class="img"
							src="https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_footer-2560.jpg"
							width="2560"
							height="800"
							alt="">
					</picture>

					<div class="extra">
						<picture>
							<source srcset="https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_gfx_6-640.webp 640w,
							https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_gfx_6-960.webp 960w,
							https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_gfx_6-1280.webp 1280w" type="image/webp" sizes="25vw">
							<source srcset="https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_gfx_6-640.png 640w,
							https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_gfx_6-960.png 960w,
							https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_gfx_6-1280.png 1280w" type="image/png" sizes="25vw">
							<img class="img-auto" src="https://s3.motionvfx.com/mvfxpublic/products/templates/1362/media/mTuber_3_gfx_6-1280.png" alt="" width="1280" height="648" />
						</picture>
					</div>
				</div>
				<div class="container">
					<div class="text-box">
						<h2>Make yourself a YouTube channel</h2>
						<p>…users love to subscribe to! mTuber 3 is not merely a collection of practical elements that make any video more fun. With a wealth of intros, transitions, supers, icons and so much more, this plugin is your ultimate choice for all things YouTube. It’s showcase time, baby!</p>
						{# {% with discount_text='INTRO', version=2 %}
						{% include 'product/buy_button.jinja2' %}
						{% endwith %} #}
					</div>
				</div>
			</section>

	</div>

    {% include './components/submenu.jinja' %}

    <script src="scripts/vendor/splitting-lite.min.js"></script>
	<script src="dist/scripts.js"></script>
    <script src="scripts/vendor/menu.js" defer></script>
    <script src="scripts/vendor/scroll_to_anchor.js" defer></script>

	{# <link href="css/vendor/popup-left.css" rel="stylesheet" /> #}
	{# <script src="scripts/vendor/popup-tech-spec.js" defer></script> #}
</body>
</html>
