    {% macro picture(
        media_url,
        filename,
        media_files={},
        file_sizes=[360, 480, 640, 960],
        formats=['webp','jpg'],
        media_sizes='',
        media='',
        alt='',
        img_class='',
        img_style='',
        picture_class='',
        width='',
        height='',
        loading='',
        data_src=False
    ) %}
        <picture class="{{ picture_class }}">
            {% for media in media_files %}
                {% set m = media_files[media] %}
                {% for format in formats %}
                    <source {% if data_src %} data-srcset=" {% for size in m['file_sizes'] %} {{ media_url }}/{{ m['filename'] }}-{{ size }}.{{ format }} {{ size }}w,{% endfor %}
                            {% else %}
                            srcset=" {% for size in m['file_sizes'] %} {{ media_url }}/{{ m['filename'] }}-{{ size }}.{{ format }} {{ size }}w,{% endfor %}"
                            {% endif %}
                            type="image/{{ format | replace('jpg', 'jpeg') }}"
                            sizes="{{ m['media_sizes'] | default(media_sizes) }}"
                            media="{{ media }}">
                {% endfor %}
            {% endfor %}
            {% for format in formats %}
                <source {% if data_src %} data-srcset=" {% for size in file_sizes %} {{ media_url }}/{{ filename }}-{{ size }}.{{ format }} {{ size }}w,{% endfor %}
                        {% else %}
                        srcset=" {% for size in file_sizes %} {{ media_url }}/{{ filename }}-{{ size }}.{{ format }} {{ size }}w,{% endfor %}"
                        {% endif %}
                        type="image/{{ format | replace('jpg', 'jpeg') }}"
                        sizes="{{ media_sizes }}"
                        media="{{ media }}">
            {% endfor %}
            <img {% if data_src %} data-src="{{ media_url }}/{{ filename }}-{{ file_sizes[0] }}.{{ formats[-1] }}?{{ file_version }}" {% else %} src="{{ media_url }}/{{ filename }}-{{ file_sizes[0] }}.{{ formats[-1] }}?{{ file_version }}" {% endif %}
                    alt="{{ alt }}"
                    class="{{ img_class }}"
                    style="{{ img_style }}"
                    width="{{ width }}"
                    height="{{ height }}"
                    loading="{{ loading }}">
        </picture>
    {% endmacro %}

    {% macro person_section(d = {
        'section': {
                'name': 'name',
                'header': {
                    'class': 'class-title',
                    'title': 'Section title',
                    'subtitle': 'Section subtitle',
                    "deco": "<div class='letters slogan-5'>modern.</div><div class='letters letter-n'>N</div>"
                }
            },
            'persons': [
                {
                    "src": ["src", "src"],
                    "col_class": 'art-col-img-motion',
                    "name": "Placeholder name",
                    "position": "Placeholder position",
                    "deco": "<div class='letters slogan-5'>modern.</div><div class='letters letter-n'>N</div>",
                    "width_height": [320, 396]
                }
            ]
    }) %}
        <section id="{{ d.section.name }}" class="{{ d.section.name }} persons">
            {{ d.section.deco|safe }}
            <div class="container">
                <div class="{{ d.section.header.class }} section__head">
                    <p class="section__subtitle">{{ d.section.header.title }}</p>
                    <h2 class="section__title">{{ d.section.header.subtitle }}</h2>
                </div>
                <div class="row">
                    {% for p in d.persons %}
                        {% set p_index = loop.index %}
                        <div class="
                                {%- if p.class -%}
                                    {{ p.class }}
                                {%- else -%}
                                    col-lg-3 col-md-6 col-sm-6 {{ d.section.name }}-col person
                                {%- endif -%}
                            "
                            style="--s-index: {{ p_index % 5 }}">
                            {%- if p.src -%}
                                {# <div class="{{ d.section.name }}-col-img person__img"> #}
                                    <div class="{{ d.section.name }}-col-img person__img {{ p.col_class }}">
                                    {% for s in p.src %}
                                        {{ picture(
                                            media_url=media_url,
                                            filename=s,
                                            file_sizes=[420, 840],
                                            formats=['jpg'],
                                            media_sizes='',
                                            media='',
                                            alt=p.name + ', ' + p.position,
                                            img_class='lazy-show pic-' + loop.index|string,
                                            width=p.width_height[0] if p.width_height else 320,
                                            height=p.width_height[1] if p.width_height else 396,
                                            data_src=False,
                                            loading="lazy"
                                            ) }}
                                    {% endfor %}
                                    </div>
                                {# </div> #}
                                <div class="{{ d.section.name }}-col-text">
                                    <div class="name">{{ p.name }}</div>
                                    <div class="position">{{ p.position }}</div>
                                </div>
                            {%- endif -%}
                        </div>
                        {{ p.deco|safe }}
                    {% endfor %}
                </div>
            </div>
        </section>
    {% endmacro %}
