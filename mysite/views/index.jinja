        <script>
            const onHeaderImagesLoaded = (type) => {
                let images
                const isMobile = type === 'mobile'

                const header = document.querySelector('section.header')
                images = isMobile
                    ? Array.from(header.querySelectorAll('.on-mobile img'))
                    : Array.from(header.querySelectorAll('.carousel-item img'))
                const loadedImages = images.map(img => false);
                images.forEach((img, idx) => {
                    loadedImages[idx] = img.complete;

                    if (loadedImages.every(val => val)) {
                        header.classList.add("loaded")
                    }
                    img.onload = () => {
                        loadedImages[idx] = true
                        if (loadedImages.every(val => val)) {
                            header.classList.add("loaded")
                        }
                    }
                })
            }

            const mM = (x) => {
                if (x.matches) {
                    onHeaderImagesLoaded('mobile')
                } else {
                    onHeaderImagesLoaded('desktop')
                }
            }
            const wMQ = window.matchMedia("(max-width: 960px)")
            mM(wMQ);
            wMQ.addListener(mM);
        </script>

