            <section class="minstaller__section">
                <div class="container">
                    <div class="row">
                        <div class="col">
                            <div class="minstaller">
                                <div class="minstaller__title">mInstaller</div>
                                <p class="minstaller__description">
                                    After purchasing, the product is <b>instantly available </b>for download via <a href="https://www.motionvfx.com/page/minstaller.html" target="_blank" rel="noopener noreferrer">mInstaller</a>.
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

        </section>

        <footer class="footer">
            <div class="image__background--carousel secuence" style="--s-index:0">
                {% set image_name = 'footer' %}
                <picture class="on-mobile">
                    <img data-src="{{mediaUrl}}/{{media_prefix}}{{image_name}}-m-raw.jpg">
                </picture>

                <div class="carousel slide carousel-fade fade-new" data-ride="carousel" data-interval="3000" data-pause="false">
                    <div class="carousel-inner">
                        {% for i in ['_a', '_b'] %}
                            {% set active = 'active' if loop.index==1 else '' %}
                            {{picture(
                                media_url=mediaUrl,
                                filename= media_prefix + image_name + i,
                                file_sizes=srcsetSizes,
                                formats=['jpg', 'webp'],
                                picture_class='carousel-item ' + active,
                                width=2560,
                                height=860,
                                data_src=True
                            )}}
                        {% endfor %}
                    </div>
                </div>

            </div>
            <div class="section__content footer__content">
                <div class="container">
                    <div class="row">
                        <div class="col-12">
                            <div class="section__text__content">
                                <div class="section__head">
                                    <div class="footer__title section__title">
                                        {{data.footer.title}}
                                    </div>
                                    <p class="footer__text section__text secuence">
                                        {{ data.footer.text }}
                                    </p>
                                </div>
                                <div class="button-buy">
                                    {% with discount_text="INTRO", version=2 %}
                                        {% include "product/buy_button.jinja2" %}
                                    {% endwith %}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </main>

    {# {% include './components/popup-left.jinja' %} #}
    {% include './components/submenu.jinja' %}

    <script src="dist/scripts.js"></script>
</body>

</html>
