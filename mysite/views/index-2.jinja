                            type="image/jpeg"
                            sizes="100vw"
                        >
                        <img
                            class="secuence"
                            width="2560"
                            height="1130"
                            src="{{mediaUrl}}/{{image_name}}-2560.jpg"
                            sizes="100vw">
                    </picture>
                </div>
                <div class="container">
                    <div class="row">
                        <div class="col-12">
                            <div class="section__head">
                                <div class="section__subtitle secuence">
                                    {{data.prevs.subtitle2}}
                                </div>
                                <div class="section__title">
                                    {{data.prevs.title2}}
                                </div>
                                <div class="section__text secuence">
                                    {{data.prevs.text2}}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>

        <a class="nav" href="#" name="specs" rel="noopener noreferrer"></a>
        <div class="ts-mi bg--dark">

            <section class="tech-spec">
                <div class="container">
                    <div class="row">
                        <div class="col-12">
                            <div class="tech-spec-info">
                                <div class="section__text__content">
                                    <div class="section__head">
                                        <div class="section__title tech-spec__title section__title">
                                            Technical specifications
                                        </div>
                                    </div>
                                    <div class="ttable tech-spec__table">
