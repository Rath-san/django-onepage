                </picture>
            </div>
            <div class="section__content footer__content">
                <div class="container">
                    <div class="row">
                        <div class="col-12">
                            <div class="section__text__content">
                                <div class="section__head">
                                    <div class="footer__title section__title">
                                        {% autoescape off %} {{data.footer.title}} {% endautoescape %}
                                    </div>
                                    <p class="footer__text section__text secuence">
                                        {{ data.footer.text }}
                                    </p>
                                </div>
