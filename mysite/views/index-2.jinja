                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        {% endwith %}

        {% with sections = [
            [{
                'images': [
                    ['boots_after-1060', 'boots-1060'],
                    ['car_after-1060', 'car-1060'],
                ]
            },{
                'images': [
                    ['m_slider_square_1_after-1400', 'm_slider_square_1-1400'],
                    ['m_slider_square_3_after-1400', 'm_slider_square_3-1400'],
                    ['m_slider_square_2_after-1400', 'm_slider_square_2-1400'],
                ]
            }], [{
                'images': [
                    ['m_slider_tall_1_after-1080', 'm_slider_tall_1-1080'],
                    ['m_slider_tall_2_after-1080', 'm_slider_tall_2-1080'],
                    ['m_slider_tall_3_after-1080', 'm_slider_tall_3-1080'],
                ]
            }]
        ] %}
        <section class="promo promo-mosaic">
            {% for slides in sections %}
                {% set section_index = loop.index %}
                <div class="mosaic-comp mosaic-comp--{{section_index}}">
                {% for slide in slides %}
                {% set slidesIndex = loop.index %}
                <div id="carouselMosaic{{section_index}}{{slidesIndex}}" class="secuence carousel mosaic-item--{{section_index}}-{{slidesIndex}} slide">
                    {% if slidesIndex == 2 %}
                        <div class="mosaic-info secuence">hover or tap to see <b>before – after</b></div>
                    {% endif %}
                    <div class="carousel-inner">
                        {% for image in slide.images %}
                        {% set imageIndex = loop.index %}

                        <div class="carousel-item carousel-item--{{imageIndex}} {% if imageIndex == 1 %}active{% endif %}">
                            {% if image is iterable and image is not string %}
                                {% for img in image %}
                                    <picture class="mosaic-img--{{imageIndex}} image-{% if loop.index == 2 %}before{% else %}after{% endif %}">
                                        <source media=""
                                            srcset="
                                                {{mediaUrl}}/{{img}}.webp,
                                            "
                                            type="image/webp"
                                            sizes="100vw"
                                        >
                                        <img
                                            width="2560"
                                            height="1100"
                                            src="{{mediaUrl}}/{{img}}.jpg"
                                            sizes="100vw">
                                    </picture>
