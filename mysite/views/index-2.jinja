                                <div class="section__title section__title--big {{ 'left' if loop.index % 2 != 0 else 'left' }}">{{ category.label }}</div>
                                <div class="row">

                                {% for i in range(category.count) %}
                                {% set num = '%02d' % loop.index %}
                                {% set filename = 'https://s3.motionvfx.com/mvfxpublic/products/templates/' + productPageTemplateId + '/media/' + key + num %}
