            {% set collapseAfter = 10 %}
            {% set namePrefix = '' %}
            {% set categories = [
                    { 'key':'CCTV', 'label': 'CCTV', 'count': 10},
                    { 'key':'Dreamy', 'label': 'Dreamy', 'count': 10},
                    { 'key':'Glitch', 'label': 'Glitch', 'count': 10},
                    { 'key':'Graphic', 'label': 'Graphic', 'count': 10},
                    { 'key':'Retro', 'label': 'Retro', 'count': 10},
