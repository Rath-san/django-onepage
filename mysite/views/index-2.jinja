                                {% endfor %}
                            "
                            type="image/webp"
                            sizes="100vw"
                        >
                        <source media=""
                            srcset="
                                {% for size in srcsetSizes %}
                                    {{mediaUrl}}/{{slide_img.src}}-{{size}}.jpg {{size}}w,
                                {% endfor %}
                            "
                            type="image/jpeg"
                            sizes="100vw"
                        >
                        <img
                            class="w-100 d-block"
                            width="2560"
                            height="950"
                            src="{{mediaUrl}}/{{slide_img.src}}-2560.jpg?{{refresh}}"
