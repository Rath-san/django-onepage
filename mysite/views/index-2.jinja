            'title': 'Let’s make things interesting!',
            'text': 'Breathe a new spirit into your editing job by applying a set of these exceptional footage effects. Redefine the videos and captivate your audience with thrilling visualizations that make your work truly stand out and astonish.'
        }
    } %}

    {% set header_comps = [
        [
            'woman',
            'hand',
            'circle_red',
            'circle_yellow',
        ],
        [
            'face_2_square',
            'face_square',
            'purple_square',
            'red_square',
            'man',
        ]
    ] %}

    <main>
        <section class="header text-center">
            <img src="https://s3.motionvfx.com/mvfxpublic/products/templates/1384/media/header-2332.webp" style="display: none;" alt="">
            <div class="header__bg header-comp" style="--h-index: 0;">
                    <picture class="comp-bg">
                    {% set image_name = 'header_bg' %}
                    <source media=""
                        srcset="
                            {% for size in srcsetSizes %}
                                {{mediaUrl}}/{{image_name}}-{{size}}.webp {{size}}w,
                            {% endfor %}
                        "
                        type="image/webp"
                        sizes="100vw"
                    >
                    <source media=""
                        srcset="
                            {% for size in srcsetSizes %}
                                {{mediaUrl}}/{{image_name}}-{{size}}.jpg {{size}}w,
                            {% endfor %}
                        "
                        type="image/jpg"
                        sizes="100vw"
                    >
                    <img
                        width="2560"
                        height="860"
                        src="{{mediaUrl}}/{{image_name}}-2560.jpg"
                        sizes="100vw">
                </picture>
                {% for comp in header_comps %}
                    {% set comp_index = loop.index %}
                    <div class="comp">
                        {% for comp_name in comp %}
                            <img
                                class="comp-img {{comp_name}} {% if comp_name == 'woman' or comp_name == 'man' %}static{% endif %}"
                                width="1"
                                height="1"
                                src="{{mediaUrl}}/header-comp-ph.png"
                                style="--h-index: {{loop.index}}"
                                sizes="">
                            {% if comp_index == 2 and loop.index == 2 %}
                                <div class="comp-img solid_circle" style="--h-index: 5"></div>
                            {% endif %}
                        {% endfor %}
                    </div>
                {% endfor %}
            </div>
            <div class="container-fluid">
                <div class="header__content section__text__content">
                    <div class="header__logo" data-title="true" style="--h-index: 0;">
                        <h2 class="logotype">
                            mRestyle
                        </h2>
                    </div>
                </div>
                <h1 class="header__subtitle section__text" style="--h-index: 1;">
                    {% autoescape off %} {{product.head_info}} {% endautoescape %}
                </h1>
                <div class="header__deco" style="--h-index: 2;">
                    <img src="https://s3.motionvfx.com/mvfxpublic/software/DV_color_icon.png" width="50" height="50" alt="" style="--h-index: 3;">
                </div>
            </div>

        </section>
        <div class="dr-gradient" style="--h-index: 4"></div>

        <script>
            const onHeaderImagesLoaded = () => {
                const header = document.querySelector('section.header')
                const images = Array.from(header.querySelectorAll('img'));
                const loadedImages = images.map(img => false);
                images.forEach((img, idx) => {
                    loadedImages[idx] = img.complete;

                    if (loadedImages.every(val => val)) {
                        header.classList.add("loaded")
                    }
                    img.onload = () => {
                        loadedImages[idx] = true
                        if (loadedImages.every(val => val)) {
                            header.classList.add("loaded")
                        }
                    }
                })
            }
            onHeaderImagesLoaded();
        </script>

        <div class="scroll-prompt" style="--h-index: 2; opacity: 0;">
            <a class="scroll-to-btn" href="#" onclick="scrollToAnchor('trailer');" tabindex="-1"></a>
        </div>

        <a class="nav" name="trailer"></a>

        {% set trailerUrl = 'https://www.youtube.com/embed/nKAZqKYv9e0?rel=0' %}

        <section class="video trailer">
