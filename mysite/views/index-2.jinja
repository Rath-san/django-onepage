                <picture>
                    {% set image_name = 'footer_bg' %}
                    <source media=""
                        srcset="
                            {% for size in srcsetSizes %}
                                {{mediaUrl}}/{{image_name}}-{{size}}.webp {{size}}w,
                            {% endfor %}
                        "
                        type="image/webp"
                        sizes="100vw"
                    >
                    <source media=""
                        srcset="
                            {% for size in srcsetSizes %}
                                {{mediaUrl}}/{{image_name}}-{{size}}.jpg {{size}}w,
                            {% endfor %}
                        "
                        type="image/jpeg"
                        sizes="100vw"
                    >
                    <img
                        width="2560"
                        height="860"
