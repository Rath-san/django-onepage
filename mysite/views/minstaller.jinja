<script>
    function getOS() {
    var userAgent = window.navigator.userAgent,
        platform = window.navigator.platform,
        macosPlatforms = ['Macintosh', 'MacIntel', 'MacPPC', 'Mac68K'],
        windowsPlatforms = ['Win32', 'Win64', 'Windows', 'WinCE'],
        iosPlatforms = ['iPhone', 'iPad', 'iPod'],
        os = null;
    
    if (macosPlatforms.indexOf(platform) !== -1) {
        os = 'macOS';
    } else if (iosPlatforms.indexOf(platform) !== -1) {
        os = 'iOS';
    } else if (windowsPlatforms.indexOf(platform) !== -1) {
        os = 'Windows';
    } else if (/Android/.test(userAgent)) {
        os = 'Android';
    } else if (!os && /Linux/.test(platform)) {
        os = 'Linux';
    }
    return os;
    }
    const os = getOS();
    // if (os === 'macOS') document.querySelector('#btn-windows').classList.add('d-none');
    // if (os === 'Windows') document.querySelector('#btn-macos').classList.add('d-none');
    
    const osSpecificButtons = {
        windows: document.querySelectorAll(`[data-os='win']`),
        mac: document.querySelectorAll(`[data-os='mac']`),
    }
    
    osSpecificButtons.mac.forEach(btn => {
        setTimeout(() => btn.classList.remove('loading'), 1400)
    })
    osSpecificButtons.windows.forEach(btn => {
        setTimeout(() => btn.classList.remove('loading'), 1400)
    })
    if (os === 'Windows') {
        osSpecificButtons.windows.forEach(btn => {
            btn.classList.replace('btn__outline--none', 'btn__fill--primary')
        })
        osSpecificButtons.mac.forEach(btn => {
            btn.classList.replace('btn__outline--none', 'btn__outline--gray')
        })
    }
    else {
        osSpecificButtons.mac.forEach(btn => {
            btn.classList.replace('btn__outline--none', 'btn__fill--primary')
        })
        osSpecificButtons.windows.forEach(btn => {
            btn.classList.replace('btn__outline--none', 'btn__outline--gray')
        })
    } 
</script>
