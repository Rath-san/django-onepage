<!DOCTYPE html>
<html lang="en">
{% include './components/head.jinja' %}
<body>
    <style>
    .shop__buttons .btn.loading {
        transition: all 1s .4s;
    }
    .btn__outline--none {
        box-shadow: none;
        border-color: transparent;
    }
    .btn__outline--gray {
        opacity: .7;
        box-shadow: none;
        border-color: transparent;
        transition: all .25s;
    }
    
    .btn__outline--gray:hover {
        border-color: #ffffff;
        box-shadow: 3px inset #ffffff;
    }
    @media (pointer: coarse) {
        .btn__outline--gray {
            border-color: #ffffff;
            box-shadow: 3px inset #ffffff;
        }
    }
    </style>
    {% include './components/nav.jinja' %}
        <!-- Main content -->
    {% block content %}

    {% macro picture_w_placeholder(
		media_url, filename, media_files={}, file_sizes=[360, 480, 640, 960], formats=['webp','jpg'], media_sizes='', media='', alt='', placeholder='', img_class='', img_style='', picture_class='', width='', height='', loading='', data_src=False
	) %}
        {% if placeholder %}
            <div class="placeholder"
                style=" padding-bottom: calc({{ height }} / {{ width }} * 100%); background: url({{ placeholder }}) center no-repeat; background-size: cover; ">
            </div>
        {% endif %}
		<picture class="{{ picture_class }}">
			{% for media in media_files %}
				{% set m = media_files[media] %}
				{% for format in formats %}
					<source {% if data_src %} data-srcset=" {% for size in m['file_sizes'] %} {{ media_url }}/{{ m['filename'] }}-{{ size }}.{{ format }} {{size}}w, {% endfor %}" {% else %} srcset=" {% for size in m['file_sizes'] %} {{ media_url }}/{{ m['filename'] }}-{{ size }}.{{ format }} {{size}}w, {% endfor %}" {% endif %} type="image/{{ format | replace('jpg', 'jpeg') }}" sizes="{{ m['media_sizes'] | default(media_sizes) }}" media="{{ media }}" >
				{% endfor %}
			{% endfor %}
			{% for format in formats %}
				<source {% if data_src %} data-srcset=" {% for size in file_sizes %} {{ media_url }}/{{ filename }}-{{ size }}.{{ format }} {{size}}w, {% endfor %}" {% else %} srcset=" {% for size in file_sizes %} {{ media_url }}/{{ filename }}-{{ size }}.{{ format }} {{size}}w, {% endfor %}" {% endif %} type="image/{{ format | replace('jpg', 'jpeg') }}" sizes="{{ media_sizes }}" media="{{ media }}">
			{% endfor %}
			<img {% if data_src %} data-src="{{ media_url }}/{{ filename }}-{{ file_sizes[0] }}.{{ formats[-1] }}?{{file_version}}" {% else %} src="{{ media_url }}/{{ filename }}-{{ file_sizes[0] }}.{{ formats[-1] }}?{{file_version}}" {% endif %} alt="{{alt}}" class="{{img_class}}" style="{{img_style}}" width="{{width}}" height="{{height}}" loading="{{loading}}" >
		</picture>
	{% endmacro %}

    {% macro tile(tile = {
        'name': 'ptl_01',
        'width': '1060',
        'height': '940',
        'placeholder': 'data:image/jpeg;base64,',
    }) %}
        <div class="tile__wrapper">
            <div class="tile object">
                <div class="mesh">
                    <div class="face-front">
                        <div class="img-lazy">
                            {{ picture_w_placeholder(
                                media_url,
                                filename=tile.name,
                                media_files={},
                                file_sizes= tile.file_sizes or [320, 640, 960, 1060],
                                formats=tile.fotmats or ['jpg'],
                                media_sizes='(max-width: 1024px) 50vw, 100vw',
                                alt='Floating image tile with plugin',
                                placeholder=tile.placeholder,
                                width=tile.width,
                                height=tile.height,
                                data_src=True
                            ) }}
                        </div>
                    </div>
                </div>
                <div class="shadow"></div>
            </div>
        </div>
    {% endmacro %}

    {% macro shop_buttons() %}
        <div class="shop__buttons">
            <a class="btn btn__outline--none loading"
                id="btn-macos"
                href="https://s3.motionvfx.com/mvfxpublic/mInstaller/mInstaller.dmg"
                target="_blank"
                rel="noopener noreferrer"
                data-os="{% if not magic_link %}mac{% endif %}">

                <i class="button__icon icon--apple">
                    <svg viewbox="0 0 23 28" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M22.2 19.3C21.3 18.8 19.5 17.7 19.5 14.8C19.5 12.4 21.1 11.2 22 10.5L22.6 10L22.2 9.40002C20.4 6.70002 17.3 6.40002 16.3 6.40002C15 6.40002 13.8 6.90002 12.9 7.20002C12.5 7.40002 12 7.60002 11.8 7.60002C11.5 7.60002 10.8 7.40002 10.2 7.10002C9.3 6.80002 8.3 6.40002 7.4 6.40002C5.4 6.40002 0 7.90002 0 15C0 19.5 3.5 28 7.6 28C8.6 28 9.6 27.7 10.4 27.4C11 27.2 11.6 27 12 27C12.9 27 13.4 27.2 14 27.5C14.6 27.8 15.3 28 16.3 28C20.3 28 23 20.7 23 20.3C23 19.7 22.6 19.5 22.2 19.3Z" fill="white"></path>
                        <path d="M16.6998 0C14.0998 0 10.9998 3.2 11.4998 6.1C14.1998 6.3 16.7998 3.7 16.6998 0Z" fill="white"></path>
                    </svg>
                </i>
                Get for Mac
            </a>
            <a class="btn btn__outline--none btn--windows loading"
                id="btn-windows"
                href="https://s3.motionvfx.com/mvfxpublic/mInstaller/win/mInstaller_setup.exe"
                target="_blank"
                rel="noopener noreferrer"
                data-os="{% if not magic_link %}win{% endif %}">
                <i class="button__icon icon--windows">
                    <svg viewbox="0 0 28 27" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M0 12.2H12.2V1.90002L0 3.50002V12.2Z" fill="white"></path>
                        <path d="M14 1.7V12.2H28V0L14 1.7Z" fill="white"></path>
                        <path d="M14 24.5L28 26.2V14H14V24.5Z" fill="white"></path>
                        <path d="M0 22.7L12.2 24.3V14H0V22.7Z" fill="white"></path>
                    </svg>
                </i>
                Get for Windows
            </a>
        </div>
    {% endmacro %}

    {% set template_id = '55' %}
    {% set media_url = 'https://s3.motionvfx.com/mvfxpublic/pages/templates/%s/media' % (template_id)  %}

    {% set src_set = [ 320, 640, 960, 1200, 1800, 2400 ] %}

<main class="mInstaller">
    <section class="header text--light text-center">
        <div class="header__bg"></div>
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="header__content section__text__content">
                        <div class="header__logo">
                            <h1 class="header__title">mInstaller</h1>
                            <p class="header__subtitle">
                                Your MotionVFX cloud.
                            </p>
                        </div>
                        <div class="header__buttons">
                            {% if magic_link %}
                            <div class="shop__buttons">
                                <a class="btn btn__fill--primary"
                                    id="btn-open-minstaller"
                                    href="{{ magic_link }}"
                                    target="_blank"
                                    rel="noopener noreferrer"
                                    data-os="">Open mInstaller
                                </a>
                            </div>
                            {% endif %}
                            {{ shop_buttons() }}
                        </div>
                        <p class="header__text">
                            Access your entire MotionVFX Plugins and Motion Templates collection with&nbsp;a&nbsp;click.
                        </p>
                        <a class="header__link link--yellow"
                            href="#"
                            rel="noopener noreferrer"
                            onclick="scrollToAnchor('installation');">Watch more
                        </a>
                    </div>
                    <div class="header__promo animated animation--fade animation--bottom">
                        <div class="img-lazy">
                            {{ picture_w_placeholder(
                                media_url,
                                filename='main_screen_o',
                                media_files={},
                                file_sizes=src_set,
                                formats=['jpg'],
                                media_sizes='200vw',
                                media='',
                                alt='Main interface of mInstaller aplication with available plugins',
                                placeholder='data:image/jpeg;base64,',
                                img_class='',
                                img_style='',
                                picture_class='',
                                width='2560',
                                height='1460',
                                loading='',
                                data_src=True
                            ) }}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <a name="installation" href="#" rel="noopener noreferrer"></a>

    <section class="installation bg--light text-center" style="margin-top: -1px;">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="installation__content section__text__content">
                        <h2 class="installation__title section__title">Installation tutorial</h2>
                        <p class="installation__text section__text">
                            Learn how to download, install and use mInstaller to have the best experience with MotionVFX content.
                        </p>
                    </div>
                    <div class="installation__promo">
                        <iframe src="https://www.youtube.com/embed/hF4LqU98ADY?rel=0"
                                frameborder="0"
                                allow="autoplay; encrypted-media"
                                allowfullscreen=""
                                title="mInstaller"
                                style="background-color: #000"></iframe>
                    </div>
                </div>
            </div>
        </div>
    </section>

